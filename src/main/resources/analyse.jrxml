<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="AnalyseReport" language="java" pageWidth="595" pageHeight="842" whenNoDataType="AllSectionsNoDetail">
    <queryString>
        <![CDATA[]]>
    </queryString>
    <field name="analyseName" class="java.lang.String"/>
    <field name="typeAnalyseName" class="java.lang.String"/>
    <field name="testName" class="java.lang.String"/>
    <field name="plageNormaleMax" class="java.lang.Double"/>
    <field name="plageNormaleMin" class="java.lang.Double"/>
    <field name="resultat" class="java.lang.String"/>
    <field name="uniteMesure" class="java.lang.String"/>
    <field name="status" class="java.lang.String"/>

    <!-- Title -->
    <title>
        <band height="30">
            <staticText>
                <reportElement x="0" y="0" width="100" height="30" backcolor="red"/>
                <text><![CDATA[Analyse Report]]></text>
            </staticText>
        </band>
    </title>

    <!-- Detail band -->
    <detail>
        <!-- Analyse Result -->
        <band height="20">
            <textField>
                <reportElement x="0" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{analyseName}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="100" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{typeAnalyseName}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="200" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{testName}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="300" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{plageNormaleMax}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="400" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{plageNormaleMin}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="500" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{resultat}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="600" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{uniteMesure}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="700" y="0" width="100" height="20"/>
                <textFieldExpression><![CDATA[$F{status}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>
